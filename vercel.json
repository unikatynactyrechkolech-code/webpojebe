{
  "buildCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/sluzby", "destination": "/sluzby.html" },
    { "source": "/cenik", "destination": "/cenik.html" },
    { "source": "/kontakt", "destination": "/kontakt.html" },
    { "source": "/o-nas", "destination": "/o-nas.html" },
    { "source": "/pruvodce", "destination": "/pruvodce.html" },
    { "source": "/reference", "destination": "/reference.html" },
    { "source": "/checkout", "destination": "/checkout.html" },
    { "source": "/success", "destination": "/success.html" },
    { "source": "/api/:path*", "destination": "/api/:path*" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ]
}
